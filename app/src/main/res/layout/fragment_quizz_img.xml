<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ouzin.drapeaux.fragments.QuizzImgFragment"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <!-- TODO: Update blank fragment layout -->

    <ProgressBar
        android:id="@+id/progressBar2"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="353dp"
        android:layout_height="80dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="141dp"
        android:layout_weight="1"
        android:minHeight="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewImg4"
        tools:ignore="InefficientWeight"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/imageViewImg2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="17dp"
        android:layout_weight="1"
        android:adjustViewBounds="false"
        android:baselineAlignBottom="false"
        app:layout_constraintBottom_toTopOf="@+id/imageViewImg4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewImg1"
        app:layout_constraintTop_toBottomOf="@+id/buttonCountryToFind"
        app:srcCompat="@android:drawable/editbox_dropdown_light_frame"
        tools:ignore="ContentDescription" />

    <Button
        android:id="@+id/buttonCountryToFind"
        android:layout_width="362dp"
        android:layout_height="73dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="25dp"
        android:layout_weight="1"
        android:text="@string/buttoncountrytofind"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageViewImg2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="InefficientWeight" />

    <ImageView
        android:id="@+id/imageViewImg4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="17dp"
        android:layout_weight="1"
        android:adjustViewBounds="false"
        android:baselineAlignBottom="false"
        app:layout_constraintBottom_toTopOf="@+id/progressBar2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewImg3"
        app:layout_constraintTop_toBottomOf="@+id/imageViewImg2"
        app:srcCompat="@android:drawable/editbox_dropdown_light_frame"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageViewImg1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="30dp"
        android:layout_weight="1"
        android:contentDescription=""
        app:layout_constraintBottom_toBottomOf="@+id/imageViewImg2"
        app:layout_constraintEnd_toStartOf="@+id/imageViewImg2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageViewImg2"
        app:srcCompat="@android:drawable/editbox_dropdown_light_frame"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageViewImg3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="17dp"
        android:layout_weight="1"
        android:contentDescription=""
        android:verticalScrollbarPosition="right"
        app:layout_constraintBottom_toTopOf="@+id/progressBar2"
        app:layout_constraintEnd_toStartOf="@+id/imageViewImg4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewImg1"
        app:srcCompat="@android:drawable/editbox_dropdown_light_frame"
        tools:ignore="ContentDescription" />

</androidx.constraintlayout.widget.ConstraintLayout>
